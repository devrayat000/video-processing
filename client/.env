# AWS Configuration for S3 Video Access
# These credentials are used to generate presigned URLs for video playback
VITE_AWS_REGION=us-east-1
VITE_AWS_ACCESS_KEY_ID=minioadmin
VITE_AWS_SECRET_ACCESS_KEY=minioadmin
VITE_AWS_S3_BUCKET=videos
VITE_AWS_S3_ENDPOINT=http://host.docker.internal:9000
VITE_API_BASE_URL=http://host.docker.internal:8080